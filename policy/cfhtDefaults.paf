#<?cfg paf policy ?>

calibrationDb: "calibDatabase4.paf"
rawTemplate: "%(field)s/%(obsid)d/%(exposure)d/raw-%(obsid)06d-e%(exposure)03d-c%(ccd)03d-a%(amp)03d.fits"
datatypePolicy: @cfhtDatatype.paf
registryPolicy: {
    globString: "*/*/*/raw-*.fits"
    fieldList: "field" "obsid" "exposure" "ccd" "amp"
    pathTemplate: "/(\w+)/(\d+)/(\d+)/raw-\d+-e\d+-c(\d+)-a(\d+)\.fits"
    intFields: false true true true true
}
